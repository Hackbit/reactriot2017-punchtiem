{
  "name": "moment-run",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@haroenv/react-foursquare": "^1.1.0",
    "antd": "2.11.1",
    "eslint-config-react-app": "^1.0.4",
    "firebase": "^4.1.3",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "mapbox-gl": "^0.38.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-mapbox-gl": "^2.0.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "flow-bin": "^0.48.0",
    "husky": "^0.14.1",
    "less": "^2.7.2",
    "less-watch-compiler": "^1.6.7",
    "lessc-each": "^1.1.0",
    "lint-staged": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.4.4",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "build-css": "concurrently 'lessc-each src/ src/' 'lessc-each src/components src/components'",
    "watch-css": "npm run build-css && concurrently 'less-watch-compiler src/ src/' 'less-watch-compiler src/components src/components'",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{*.js,*.less}": [
      "prettier --single-quote=true --trailing-comma=es5 --write src/**/{*.js,*.less}",
      "git add"
    ]
  }
}
